{
	"name": "@ctx-core/dialog-ui-solid",
	"version": "2.3.88",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./ui"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.jsx",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
