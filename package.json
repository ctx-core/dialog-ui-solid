		"@ctx-core/ui-solid": "^21.0.81",
		"ctx-core": "^5.12.0",
		"solid-js": "1.8.7"
	},
	"devDependencies": {
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.7.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	}
}