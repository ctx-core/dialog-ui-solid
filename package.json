{
	"name": "@ctx-core/dialog-ui-solid",
	"version": "2.3.227",
	"keywords": [
		"ctx-core",
		"dialog"
	],
	"homepage": "https://github.com/ctx-core/dialog-ui-solid#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/dialog-ui-solid/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/dialog-ui-solid.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Brian Takita",
		"url": "https://briantakita.me",
		"email": "info+ctx-core-dialog-ui-solid@briantakita.me"
	},
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./ui"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.jsx",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm run /^test:/",
		"test:types": "check-dts",
		"test:unit": "uvu . '\\.test\\.(ts|tsx|js|jsx)$'",
		"disable:test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"@ctx-core/dom": "^15.3.95",
		"@ctx-core/nanostores": "^10.0.150",
		"@ctx-core/solid-js": "^1.5.210",
		"@ctx-core/ui-solid": "^21.0.216",
		"ctx-core": "^7.2.25",
		"solid-js": "1.8.18"
	},
	"devDependencies": {
		"c8": "^10.1.2",
		"check-dts": "^0.8.0",
		"tsx": "^4.15.9",
		"typescript": "next",
		"uvu": "^0.5.6"
	}
}